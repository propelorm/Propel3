{
    "name" : "propel/propel",
    "type" : "library",
    "description" : "High performance data-mapper ORM with optional active-record traits for RAD and modern PHP 7.1+",
    "keywords" : [
        "ORM",
        "persistence",
        "Active Record",
        "data mapper"
    ],
    "homepage" : "http://www.propelorm.org/",
    "license" : "MIT",
    "authors" : [{
            "name" : "Marc J. Schmidt",
            "email" : "marc@marcjschmidt.de"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "symfony/yaml": "^3.0|^4.0",
        "symfony/console": "^3.0|^4.0",
        "symfony/finder": "^3.0|^4.0",
        "symfony/filesystem": "^3.0|^4.0",
        "symfony/event-dispatcher": "^3.0|^4.0",
        "symfony/config": "^3.0|^4.0",
        "psr/log": "^1.0",
        "gossi/php-code-generator": "0.5",
        "mustache/mustache": "^2.6",
        "marcj/topsort": "1.1.0",
        "bramus/monolog-colored-line-formatter": "^2.0",
        "phootwork/lang": "^0.9.0",
        "phootwork/file": "^0.2.0",
        "phootwork/collection": "^1.6",
        "phootwork/json": "^1.2"
    },
    "require-dev" : {
        "monolog/monolog" : "~1.3",
        "phpunit/phpunit" : "^6.0|^7.0",
        "behat/behat" : "^3.3",
        "mikey179/vfsstream": "^1.6"
    },
    "suggest" : {
        "monolog/monolog" : "The recommended logging library to use with Propel."
    },
    "autoload" : {
        "psr-0" : {
            "Propel" : "src/"
        }
    },
    "bin" : [
        "bin/propel"
    ],
    "extra" : {
        "branch-alias" : {
            "dev-master" : "3.0-dev"
        }
    }
}
