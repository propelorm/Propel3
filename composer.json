{
    "name": "propel/propel",
    "type": "library",
    "description": "High performance data-mapper ORM with optional active-record traits for RAD and modern PHP 7.1+",
    "keywords": [
        "ORM",
        "persistence",
        "Active Record",
        "data mapper"
    ],
    "homepage": "http://www.propelorm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc J. Schmidt",
            "email": "marc@marcjschmidt.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/yaml": "^2.8|^3.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/finder": "^2.8|^3.0",
        "symfony/filesystem": "^2.8|^3.0",
        "symfony/event-dispatcher": "^2.8|^3.0",
        "symfony/config": "^2.8|^3.0",
        "psr/log": "~1.0",
        "gossi/php-code-generator": "dev-master",
        "mustache/mustache": "~2.6",
        "marcj/topsort": "1.1.0",
        "bramus/monolog-colored-line-formatter": "~2.0"
    },
    "require-dev": {
        "behat/behat": "~2.4",
        "monolog/monolog": "~1.3",
        "phpunit/phpunit": "~5.6"
    },
    "suggest": {
        "monolog/monolog": "The recommended logging library to use with Propel."
    },
    "autoload": {
        "psr-0": {
            "Propel": "src/"
        }
    },
    "bin": [
        "bin/propel"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
