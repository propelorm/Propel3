{
    "name" : "propel/propel",
    "type" : "library",
    "description" : "High performance data-mapper ORM with optional active-record traits for RAD and modern PHP 7.1+",
    "keywords" : [
        "ORM",
        "persistence",
        "Active Record",
        "data mapper"
    ],
    "homepage" : "http://www.propelorm.org/",
    "license" : "MIT",
    "authors" : [{
            "name" : "Marc J. Schmidt",
            "email" : "marc@marcjschmidt.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "symfony/yaml": "^5.0",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/config": "^5.0",
        "psr/log": "^1.0",
        "marcj/topsort": "^1.1",
        "phootwork/lang": "^2.0",
        "phootwork/collection": "^2.0",
        "phootwork/json": "^2.0",
        "phootwork/file": "^2.0",
        "susina/psr2-code-generator": "^3.0",
        "monolog/monolog": "^2.0",
        "bramus/monolog-colored-line-formatter": "^3.0"
    },
    "require-dev" : {
        "mikey179/vfsstream": "^1.6",
        "behat/behat": "^3.6",
        "phpunit/phpunit": "^9.1",
        "susina/coding-standard": "^1.2"
    },
    "autoload" : {
        "psr-4" : {
            "Propel\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Propel\\Tests\\": [
                "tests/",
                "tests/Fixtures/bookstore/build/classes/Propel/Tests/",
                "tests/Fixtures/schemas/build/classes/Propel/Tests/",
                "tests/Fixtures/quoting/build/classes/Propel/Tests/",
                "tests/Fixtures/bookstore-packaged/build/classes/Propel/Tests/"
            ],
            "Foo\\": "tests/Fixtures/namespaced/build/classes/Foo/",
            "Baz\\": "tests/Fixtures/namespaced/build/classes/Baz/"
        }
    },
    "bin" : [
        "bin/propel"
    ],
    "extra" : {
        "branch-alias" : {
            "dev-master" : "3.0-dev"
        }
    }
}
